{
    "name": "pyspark-dev",
    "dockerFile": "../docker/Dockerfile",
    "context": "..",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-toolsai.jupyter",
          "ms-python.vscode-pylance"
        ],
        "settings": {
          "python.pythonPath": "/usr/local/bin/python3"
        }
      }
    },
    "postCreateCommand": "pip install -r /app/requirements.txt"
  }  